{
    "name": "developer-platform-vscode-chat",
    "version": "0.0.0",
    "publisher": "ms-vscode",
    "displayName": "Developer Platform Chat Agent",
    "description": "Microsoft Developer Platform chat agent for VS Code",
    "repository": {
        "url": "https://github.com/Microsoft/developer-platform-vscode-chat"
    },
    "engines": {
        "vscode": "^1.84.0"
    },
    "extensionDependencies": [
        "github.copilot-chat"
    ],
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "enabledApiProposals": [
        "chat",
        "chatAgents2",
        "chatAgents2Additions",
        "chatRequestAccess",
        "chatVariables"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "download-api": "dts dev && find . -name 'vscode.proposed.*.d.ts' -exec mv {} ./src/typings \\;",
        "postinstall": "npm run download-api"
    },
    "devDependencies": {
        "@types/json-schema": "^7.0.15",
        "@types/lunr": "^2.3.5",
        "@types/node": "^20.5.9",
        "@types/vscode": "1.82.0",
        "@typescript-eslint/eslint-plugin": "^6.13.2",
        "@typescript-eslint/parser": "^6.13.2",
        "eslint": "^8.35.0",
        "typescript": "^5.3.2",
        "@vscode/dts": "^0.4.0"
    },
    "dependencies": {
        "@developer-platform/entities": "0.2.22",
        "dotenv": "^16.3.1",
        "langchain": "^0.0.203",
        "lunr": "^2.3.9"
    }
}
